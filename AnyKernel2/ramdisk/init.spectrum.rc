	/*lainnya*/
	  write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor intelliactive
	  write /sys/devices/system/cpu/cpu4/cpufreq/scaling_governor intelliactive
	  chmod 644 /sys/module/workqueue/parameters/power_efficient
	  write /sys/module/workqueue/parameters/power_efficient y
	  write /sys/module/cpu_boost/parameters/input_boost_enabled 1

	# HOTPLUG
	  write /sys/devices/system/cpu/cpu0/core_ctl/min_cpus 1
	  write /sys/devices/system/cpu/cpu0/core_ctl/busy_down_thres 40
	  write /sys/devices/system/cpu/cpu0/core_ctl/busy_up_thres 68
	  write /sys/devices/system/cpu/cpu0/core_ctl/offline_delay_ms 100
	  
	# THERMAL
	  write /sys/module/msm_thermal/parameters/enabled Y
	  write /sys/module/msm_thermal/parameters/limit_temp_degC 60
	  write /sys/module/msm_thermal/parameters/core_limit_temp_degC 80
	  write /sys/module/msm_thermal/core_control/enabled 0
	  write /sys/module/msm_thermal/vdd_restriction/enabled 0
	    
	# GPU
	  write /sys/class/kgsl/kgsl-3d0/max_gpuclk 475000000
	  write /sys/class/kgsl/kgsl-3d0/devfreq/min_freq 216000000
	  write /sys/class/kgsl/kgsl-3d0/devfreq/governor msm-adreno-tz
	  write /sys/module/simple_gpu_algorithm/parameters/simple_gpu_activate 1
	  write sys/module/simple_gpu_algorithm/parameters/simple_laziness 1
	  write /sys/module/simple_gpu_algorithm/parameters/simple_ramp_threshold 7000
	  write /sys/module/adreno_idler/parameters/adreno_idler_active 1
	  write /sys/module/adreno_idler/parameters/adreno_idler_idleworkload 4000
	  write /sys/module/adreno_idler/parameters/adreno_idler_idlewait 25
	  write /sys/module/adreno_idler/parameters/adreno_idler_downdifferential 25
	  write /sys/class/devfreq/1c00000.qcom,kgsl-3d0/adrenoboost 1

	# I/O
	  write /sys/block/mmcblk0/queue/scheduler zen
	  write /sys/block/mmcblk0/queue/read_ahead_kb 2048
	  write /sys/block/mmcblk0/queue/rotational 1
	  write /sys/block/mmcblk0/queue/iostats 0
	  write /sys/block/mmcblk0/queue/rq_affinity 2
	  write /sys/block/mmcblk0/queue/add_random 1
	  write /sys/block/mmcblk1/queue/scheduler zen
	  write /sys/block/mmcblk1/queue/read_ahead_kb 1024
	  write /sys/block/mmcblk1/queue/rotational 1
	  write /sys/block/mmcblk1/queue/iostats 0
	  write /sys/block/mmcblk1/queue/rq_affinity 2
	  write /sys/block/mmcblk1/queue/add_random 1
	    
	# VM
	  write /proc/sys/vm/dirty_ratio 30
	  write /proc/sys/vm/dirty_background_ratio 5
	  write /proc/sys/vm/laptop_mode 1
	  write /proc/sys/vm/swappiness 100
	  write /proc/sys/vm/dirty_writeback_centisecs 700
	  write /proc/sys/vm/dirty_expire_centisecs 200
	  write /proc/sys/vm/oom_kill_allocating_task 0
	  write /proc/sys/vm/page-cluster 3
	  write /proc/sys/vm/swap_ratio_enable 80
	  write /proc/sys/vm/overcommit_ratio 80
	  write /proc/sys/vm/drop_caches 0

	# LKM
	  chmod 666 /sys/module/lowmemorykiller/parameters/minfree
	  chown root /sys/module/lowmemorykiller/parameters/minfree
	  write /sys/module/lowmemorykiller/parameters/minfree 14175,18900,23625,51975,70875,75600

	# Misc
	  write /sys/kernel/dyn_fsync/Dyn_fsync_active 1
	  write /sys/kernel/sched/gentle_fair_sleepers 1
	  write /sys/kernel/sched/arch_power 1
	  write /sys/kernel/power_suspend/power_suspend_mode 3
	  write /proc/sys/net/ipv4/tcp_congestion_control westwood
	  

