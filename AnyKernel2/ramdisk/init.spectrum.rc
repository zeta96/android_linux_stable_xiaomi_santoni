	  chmod 644 /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq
	  write /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq 1401000
	  chmod 444 /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq
